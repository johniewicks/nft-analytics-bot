{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "chmod +x install.sh && ./install.sh && pip install -r requirements.txt --no-cache-dir"
  },
  "deploy": {
    "startCommand": "./start_encrypted.sh",
    "restartPolicyType": "ON_FAILURE",
    "healthCheckPath": "/health",
    "healthCheckTimeout": 100
  }
}
